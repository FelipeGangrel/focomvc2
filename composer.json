{
    "name": "slim/slim-skeleton",
    "description": "A Slim Framework skeleton application for rapid development",
    "keywords": [
        "microframework",
        "rest",
        "router",
        "psr7",
        "graphql"
    ],
    "homepage": "http://github.com/slimphp/Slim-Skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Time de Web da Foco",
            "email": "internet@fococomunicacao.com",
            "homepage": "http://www.fococomunicacao.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/database": "^5.8",
        "illuminate/filesystem": "^5.8",
        "illuminate/pagination": "^5.8",
        "illuminate/translation": "^5.8",
        "illuminate/validation": "^5.8",
        "monolog/monolog": "^1.17",
        "slim/php-view": "^2.0",
        "slim/slim": "^3.1",
        "webonyx/graphql-php": "^0.13.5"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.0"
    },
    "autoload": {
        "psr-4": {
            "Foco\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
